{
  "name": "theme-nextjs-bea",
  "version": "0.1.0",
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 7.x"
  },
  "devEngines": {
    "node": ">= 14.x",
    "npm": ">= 7.x"
  },
  "scripts": {
    "dev": "npm run intl:compile && next",
    "build": "npm run intl:compile && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-styles": "npx stylelint **.scss",
    "lint-styles:fix": "npx stylelint **.scss --fix",
    "prettier": "prettier --check './**/*.(js|ts|tsx|css)'",
    "prettier:fix": "npm run prettier -- --write --no-list-different",
    "typecheck": "tsc --noEmit --incremental",
    "check": "npm run lint && npm run typecheck && npm run lint-styles",
    "stylelint-check": "stylelint-config-prettier-check",
    "intl:extract": "formatjs extract '{components,modules,pages,utils}/**/*.ts*' --out-file lang/source/en.json --format crowdin --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile": "formatjs compile-folder --ast --format crowdin lang/source lang/compiled"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^0.3.7",
    "@headlessui/react": "^1.4.1",
    "@prezly/sdk": "^3.4.0",
    "@prezly/slate-renderer": "^0.2.1",
    "@prezly/slate-types": "^0.2.1",
    "@prezly/uploadcare-image": "^0.2.5",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "date-fns-timezone": "^0.1.4",
    "modern-normalize": "^1.1.0",
    "next": "^12.0.1",
    "next-seo": "^4.28.0",
    "parse-data-url": "^4.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.21.0",
    "react-share": "^4.4.0",
    "social-links": "^1.4.5"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.4.0",
    "@svgr/webpack": "^5.5.0",
    "@types/classnames": "^2.3.1",
    "@types/node": "^16.10.2",
    "@types/parse-data-url": "^3.0.0",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.3.7",
    "babel-plugin-formatjs": "^10.3.11",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-formatjs": "^2.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-export-all": "^1.1.1",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.4.3"
  },
  "license": "GNU GPL v3"
}
